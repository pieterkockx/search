{
  "manifest_version": 2,

  "name": "Search",
  "description": "Search bookmarked URLs containing %s with query in place of %s",
  "version": "0.1.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "bookmarks",
    "tabs"
  ]
}
