{
  "manifest_version": 2,

  "name": "Search",
  "description": "Fire up all search engines at your disposal with a single shot.",
  "version": "0.1.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "bookmarks",
    "tabs"
  ]
}
